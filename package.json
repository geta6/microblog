{
  "name": "epicify",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:geta6/epicify.git",
  "author": "geta6 <getakura@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "scripty",
    "start": "scripty",
    "test": "scripty",
    "test:watch": "scripty"
  },
  "scripty": {
    "silent": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{png,jpg,jpeg,gif}": [
      "imagemin-lint-staged",
      "git add"
    ],
    "*.svg": [
      "svgo --config '{\"plugins\":[{\"removeViewBox\":false},{\"removeDimensions\":true},{\"convertColors\":{\"currentColor\":true}}]}'",
      "git add"
    ],
    "*.json": [
      "jsonlint",
      "git add"
    ]
  },
  "jest": {
    "testURL": "http://localhost:3000",
    "rootDir": "./app",
    "testRegex": "__tests__/.+\\.(test|spec)\\.tsx?$",
    "bail": false,
    "verbose": false,
    "timers": "fake",
    "cacheDirectory": "<rootDir>/../tmp/jest",
    "coverageDirectory": "<rootDir>/../tmp/coverage",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      ".+\\.svg$": "<rootDir>/../lib/tests/stub/svg.js"
    }
  },
  "dependencies": {
    "express": "^4.17.3",
    "helmet": "^3.21.1",
    "mongoose": "^5.7.1",
    "next": "^9.0.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.4.0",
    "styled-reset": "^4.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/helmet": "^0.0.44",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "imagemin-lint-staged": "^0.4.0",
    "jest": "^24.9.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^9.3.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "scripty": "^1.9.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
